#include<iostream>
using namespace std;

int main()
{
    int max=INT_MIN;
    int min=INT_MAX;
    cout<<"ARRAY IS:: "<<endl;
    int b[3][3];
    for(int r=0;r<3;r++)
    {
        for(int c=0;c<3;c++)
        {
            cin>>b[r][c];
            if(b[r][c] < min)
            min=b[r][c];
            if(b[r][c] > max)
            max=b[r][c];
        }
    }
    cout<<"MAX IS :: "<<max<<endl;
    cout<<"MIN IS :: "<<min<<endl;
    
}

OUTPUT::
ARRAY IS:: 
1 2 3
4 5 6
7 8 9
MAX IS :: 9
MIN IS :: 1
