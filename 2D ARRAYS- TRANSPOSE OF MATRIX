#include<iostream>
using namespace std;
void transpose(int b[][3],int size)
{
    cout<<"TRANSPOSE IS:: "<<endl;
    for(int r=0;r<3;r++)
    {
        for(int c=0;c<3;c++)
        {
            cout<<b[c][r]<<" ";
        }
        cout<<endl;
    }
}

int main()
{
    cout<<"ARRAY IS:: "<<endl;
    int b[3][3];
    for(int r=0;r<3;r++)
    {
        for(int c=0;c<3;c++)
        {
            cin>>b[r][c];
        }
    }
    transpose(b,3);
}

OUTPUT::
ARRAY IS:: 
1 2 3
4 5 6
7 8 9
TRANSPOSE IS:: 
1 4 7 
2 5 8 
3 6 9 
