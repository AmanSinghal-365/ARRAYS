#include<iostream>
using namespace std;

int main()
{
    int key;
    cout<<"ENTER KEY :: ";
    cin>>key;
    int b[3][3];
    cout<<"ENTER ARRAY :: "<<endl;
    for(int r=0;r<3;r++)
    {
        for(int c=0;c<3;c++)
        {
            cin>>b[r][c];
        }
    }
    int flag=0;
    for(int r=0;r<3;r++)
    {
        for(int c=0;c<3;c++)
        {
            if(key==b[r][c])
            {
                flag++;
            }            
        }
    }
    if(flag==0)
    cout<<"NOT FOUND!!!";
    else
    cout<<"FOUND!!";
    
}

OUTPUT::
ENTER KEY :: 10
ENTER ARRAY :: 
1 2 3
4 5 6
7 8 9
NOT FOUND!!!
